{
  "name": "pomodoro",
  "displayName": "Pomodoro",
  "description": "Pomodoro Timer for VSCode",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.70.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pomodoro.start",
        "title": "Pomodoro Start"
      }
    ]
  },
  "dependencies": {
    "node": "^23.4.0",
    "node-notifier": "^10.0.1",
    "play-sound": "^1.1.6",
    "yandex-music-api": "^0.0.3"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "@types/node-notifier": "^8.0.5",
    "@types/play-sound": "^1.1.2",
    "@types/vscode": "^1.95.0",
    "typescript": "^5.7.2"
  }
}
